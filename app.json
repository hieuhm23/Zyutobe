{
  "expo": {
    "name": "ZyTube",
    "slug": "zytube",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zyea.zytube",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "picture-in-picture"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "backgroundModes": [
        "audio"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.zyea.zytube",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_MEDIA_PLAYBACK"
      ],
      "config": {
        "pictureInPicture": true
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-updates",
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/f51c630a-f698-412b-94cc-a6e7b45bb054",
      "checkOnLaunch": "ALWAYS",
      "requestHeaders": {
        "expo-channel-name": "production"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "eas": {
        "projectId": "f51c630a-f698-412b-94cc-a6e7b45bb054"
      }
    },
    "owner": "hieukka"
  }
}